    <%= image_tag('temple-logo-t-box.svg', class: "header-logo d-inline-flex", height:"60", alt: "temple-logo") %>
    <span class="logo-text">University Libraries</span>
  <% end %>
</nav>

<nav id="red-navbar" class="navbar navbar-header navbar-expand-sm" role="navigation">
