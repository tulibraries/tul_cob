<div id="header-navbar" class="navbar navbar-inverse navbar-static-top" role="navigation">
  <div class="<%= container_classes %>">
    <div class="navbar-header">
    <%= link_to image_tag('temple-logo-t-box.svg', class: "header-logo"), bento_search_engine_path %>
    <%= link_to "University Libraries", bento_search_engine_path, class: "header-name" %>
    </div>
  </div>
</div>

<div id="user-util-links" class="row red-navbar ">
  <div class="<%= container_classes %>">
    <div class="col-sm-12 col-md-4">
      <%= navigational_headers %>
    </div>
    <div class="col-sm-12 col-md-7 col-lg-8 offset-lg-1" id="user-util-collapse">
      <div class="navbar-right navbar-nav">
        <%= navigational_links %>
        <%= render partial: '/user_util_links' %>
      </div>
    </div>
  </div>
</div>

<div id="search-navbar" class="navbar navbar-default navbar-static-top" role="navigation">
  <div class="<%= container_classes %>">
    <%= render "search_form"  %>
  </div>
</div>
