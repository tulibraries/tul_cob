<% doc_presenter = show_presenter(document) %>
<% if document["holdings_with_no_items_display"] %>
<table class="other_libraries table table-responsive border border-tan-border mb-0">
  <% check_holdings_library_name(document).each do |library| %>
    <thead class="table-heading">
      <tr>
        <th colspan="5" ><%= library_name_from_short_code(library) %> (For Items with Limited Information)</th>
      </tr>
    </thead>
    <tbody>
      <% check_holdings_location(document, library).each do |holding| %>
        <tr>
          <td scope="row"><%= holding  %></td>
          <td scope="row"><%= check_holdings_call_number(document) %></td>
          <td scope="row"><%= render_holdings_summary(document) %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>
<% end %>
