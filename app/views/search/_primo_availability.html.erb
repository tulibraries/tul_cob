<%= bento_availability(item.custom_data) %>

<% document = item.custom_data %>

<% if document.libkey_articles_url.present? %>
  <% if document.libkey_articles_url_retracted? %>
    <%= link_to t("articles.retracted_button_html"), document.libkey_articles_url, class: "btn btn-sm bento-avail-btn bento-libkey mt-2 mx-auto pr-0", target: "_blank" %>
  <% else %>
    <%= link_to t("articles.libkey_button_html"), document.libkey_articles_url, class: "btn btn-sm bento-avail-btn bento-libkey mt-2 mx-auto pr-0", target: "_blank" %>
  <% end %>
<% end %>
