<% format_facet = @response.aggregations["format"] %>
<% unless format_facet.items.all? { |item| item.hits == "0" } %>
  <div class="container-xl bento-resource-container resource-types-container-new mt-5">
    <h2 class="bento-resource-types"><%= t("bento.resource_types_header") %></h2>
    <hr />
    <ul class="bento-resource-types-list pt-2 list-inline">
      <% format_facet.items.each do |item| %>
        <% unless item.value == "digital_collections" %>
          <li class="bento-resource-list-item list-inline-item pb-1"><%= render_bento_format_facet_value(item) %></li>
        <% end %>
      <% end %>
    </ul>
  </div>
  <% end %>
