
    <% if @results %>
      <div class="row">

        <% @results.each_pair do |key, result| %>

              <div class="col-xl-2 col-md-3 col-sm-6  bento_compartment <%= key %>">
                <h2><%= key.titleize %> </h2>
                <%= render :layout => "layouts/bento_box_wrapper", :locals => {:results => result } do %>
                    <%= bento_search result %>
                <% end %>
              </div>

        <% end %>
      </div>
    <% else %>
      <%= render("home_text") %>
    <% end %>
