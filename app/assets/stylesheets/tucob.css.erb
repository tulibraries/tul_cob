#header-navbar .navbar-brand {
  width: 300px;
  height: 67px;
  margin: 10px 21px 10px 0px;
  padding: 0;
   background: transparent url(<%= asset_path 'NewCherryLogo.jpg' %>) no-repeat top left;
  /*background: transparent url(<%= asset_path 'temple-university-logo.png' %>) no-repeat top left;*/
}

.show_page_records {
  margin-top: 25px;
}

ul {
  display: table;
  list-style: none;
  margin: 0;
  padding: 0;
}

.list_items  {
  display: table-row;
  float: left;
  list-style-type: none;
}

button.avail-button {
  margin: 5px 5px 10px 10px;
  display: inline-block;
}

dd.blacklight-availability {
  display: inline-block;
  margin-left: 5px;
  vertical-align: middle;
}

@media (min-width: 768px) {
  .document dt {
    white-space: normal;
    word-break: break-word;
    width: 170px;
  }

  .show_page_records dd {
    margin-bottom: 20px;
  }
}

.availability_iframe {
  margin: 30px;
}

.panel-heading {
  margin: 0;
}

.bl_alma_iframe {
  border: 0;
  width: 99.9%;
}
