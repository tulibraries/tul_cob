button.collapse-button .avail-label:after {
  color: $white;
  content:"\f078";
  font-family: "Font Awesome 5 Free";
  font-weight: 900;
  padding-left: 0.625rem;
}

button.collapse-button.collapsed .avail-label:after {
  color: $white;
  content:"\f054";
  font-family: "Font Awesome 5 Free";
  font-weight: 900;
  padding-left: 0.625rem;
}

.lib-guides-recommender-bento [data-toggle="collapse"] i:before{
    content: "\f078";
}

.lib-guides-recommender-bento [data-toggle="collapse"].collapsed i:before{
    content: "\f054";
}

a.booking-request:after,
a.hold-request:after,
a.digital-request:after,
a.asrs-request:after {
  color: $red;
  content:"\f054";
  font-family: "Font Awesome 5 Free";
  font-weight: 900;
  float: right;
  padding-right: 0.625rem;
}

a.booking-request[aria-expanded="true"]::after,
a.hold-request[aria-expanded="true"]::after,
a.digital-request[aria-expanded="true"]::after,
a.asrs-request[aria-expanded="true"]::after {
  content:"\f078";
}

